<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="yanti.tari_tradisional.tatra.ListVideoIII"
    android:background="#33FFFF">


    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Maluku Utara"
        android:textSize="18dp"
        android:id="@+id/btnjabar"
        android:background="#33FF33"
        android:layout_marginTop="37dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Nanggro Aceh Darussalam"
        android:textSize="18dp"
        android:id="@+id/btnJatim"
        android:background="#33FF33"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/btnjabar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Jawa Barat"
        android:textSize="18dp"
        android:id="@+id/btnJateng"
        android:background="#33FF33"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/btnJatim"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Nusa Tenggara Timur"
        android:textSize="18dp"
        android:id="@+id/btnDKI"
        android:background="#33FF33"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/btnJateng"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Papua"
        android:textSize="18dp"
        android:id="@+id/btnBangka"
        android:background="#33FF33"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/btnDKI"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Jawa Tengah"
        android:textSize="18dp"
        android:id="@+id/btnBanten"
        android:background="#33FF33"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/btnBangka"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Riau"
        android:textSize="18dp"
        android:id="@+id/btnGorontalo"
        android:background="#33FF33"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/btnBanten"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Sulawesi Barat"
        android:textSize="18dp"
        android:id="@+id/btnBali"
        android:background="#33FF33"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/btnGorontalo"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:text="1"
        android:textSize="18dp"
        android:id="@+id/btn1"
        android:background="#33FF33"
        android:layout_alignTop="@+id/btn2"
        android:layout_toLeftOf="@+id/btn2"
        android:layout_toStartOf="@+id/btn2" />

    <Button
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:text="2"
        android:textSize="18dp"
        android:id="@+id/btn2"
        android:background="#33FF33"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/btn3"
        android:layout_toStartOf="@+id/btn3" />

    <Button
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:text="3"
        android:textSize="18dp"
        android:id="@+id/btn3"
        android:background="#33FF33"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/btn4"
        android:layout_toStartOf="@+id/btn4" />

    <Button
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:text="4"
        android:textSize="18dp"
        android:id="@+id/btn4"
        android:layout_marginLeft="300dp"
        android:background="#33FF33"
        android:layout_alignParentBottom="true" />



</RelativeLayout>
