<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="yanti.tari_tradisional.tatra.VideoI"
    android:background="#33FFFF">


    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Nusa Tenggara Barat"
        android:textSize="18dp"
        android:id="@+id/btntb"
        android:background="#33FF33"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="37dp" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Jawa Timur"
        android:textSize="18dp"
        android:id="@+id/btnjwtim"
        android:background="#33FF33"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/btntb"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Papua Barat"
        android:textSize="18dp"
        android:id="@+id/btnpapuabrtt"
        android:background="#33FF33"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/btnjwtim"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="DKI Jakarta"
        android:textSize="18dp"
        android:id="@+id/btnDKI"
        android:background="#33FF33"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/btnpapuabrtt"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Sulawesi Utara"
        android:textSize="18dp"
        android:id="@+id/btnsutara"
        android:background="#33FF33"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/btnDKI"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Yogyakarta"
        android:textSize="18dp"
        android:id="@+id/btnygyakarta"
        android:background="#33FF33"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/btnsutara"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Gorontalo"
        android:textSize="18dp"
        android:id="@+id/btnGorontaloo"
        android:background="#33FF33"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/btnygyakarta"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:text="2"
        android:textSize="18dp"
        android:id="@+id/btnN2"
        android:layout_marginTop="10dp"
        android:background="#33FF33"
        android:layout_below="@+id/btnGorontaloo"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
