<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="yanti.tari_tradisional.tatra.ListVideoII"
    android:background="#33FFFF">


    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Kalimantan Selatan"
        android:textSize="18dp"
        android:id="@+id/btnkalimantansltn"
        android:background="#33FF33"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="40dp" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Nanggro Aceh Darussalam"
        android:textSize="18dp"
        android:id="@+id/btnaceh"
        android:background="#33FF33"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/btnkalimantansltn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Kalimantan Timur"
        android:textSize="18dp"
        android:id="@+id/btnkalimantantmr"
        android:background="#33FF33"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/btnaceh"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Kepulauan Riau"
        android:textSize="18dp"
        android:id="@+id/btnRiau"
        android:background="#33FF33"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/btnkalimantantmr"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Lampung"
        android:textSize="18dp"
        android:id="@+id/btnlampungg"
        android:background="#33FF33"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/btnRiau"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Bali"
        android:textSize="18dp"
        android:id="@+id/btnGorontalo2"
        android:background="#33FF33"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/btnlampungg"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:text="Maluku"
        android:textSize="18dp"
        android:id="@+id/btnMlk"
        android:background="#33FF33"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/btnGorontalo2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:text="1"
        android:textSize="18dp"
        android:id="@+id/btnN1ab"
        android:background="#33FF33"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/btnMlk"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
