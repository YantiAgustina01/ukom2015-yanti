<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="yanti.tari_tradisional.tatra.Tarian"
    android:background="#33FFFF">



    <Button
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:text="Kicir-Kicir"
        android:textSize="18dp"
        android:id="@+id/btnjabar"
        android:background="#33FF33"
        android:layout_marginTop="38dp"
        android:layout_alignTop="@+id/imageView"
        android:layout_alignLeft="@+id/btnJatim"
        android:layout_alignStart="@+id/btnJatim" />

    <Button
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:text="Sirih Kuning"
        android:textSize="18dp"
        android:id="@+id/btnJatim"
        android:background="#33FF33"
        android:layout_marginTop="38dp"
        android:layout_below="@+id/btnjabar"
        android:layout_alignLeft="@+id/btnJateng"
        android:layout_alignStart="@+id/btnJateng"
        android:layout_centerHorizontal="true"/>

    <Button
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:text="Nandak Ganjen"
        android:textSize="18dp"
        android:id="@+id/btnjtg"
        android:background="#33FF33"
        android:layout_marginTop="38dp"
        android:layout_below="@+id/btnJatim"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:text="Jali-Jali"
        android:textSize="18dp"
        android:id="@+id/btnDKK"
        android:background="#33FF33"
        android:layout_below="@+id/btnjtg"
        android:layout_alignLeft="@+id/btnjtg"
        android:layout_alignStart="@+id/btnjtg"
        android:layout_marginTop="37dp" />
</RelativeLayout>
